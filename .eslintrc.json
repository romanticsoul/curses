{
  "extends": [
    "next/core-web-vitals",
    "plugin:tailwindcss/recommended",
    "prettier"
  ],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js"],
      "parser": "@typescript-eslint/parser"
    }
  ],
  "settings": {
    "tailwindcss": {
      "callees": ["classnames", "clsx", "ctl", "tw", "twMerge", "cn", "cva"],
      "whitelist": [
        "material-symbols-outlined",
        "material-symbols-rounded",
        "material-symbols-sharp"
      ]
    }
  }
}
